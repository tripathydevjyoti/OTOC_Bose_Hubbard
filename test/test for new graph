using Plots
using PyCall

np = pyimport("numpy")

data1 = np.load("figsix_two_five.npy")
data2 = np.load("figsix_two_six.npy")
data3 = np.load("figsix_two_three.npy")

Jtimes = 4*range(0,2.0,40)
plot(Jtimes,[data1,data2,data3],linewidth=3,label=["Config 1" "Config 2" "Config3"])
xlabel!("Jt")
ylabel!("|OTOC|")
title!("Graph 4")
savefig("Graph4.pdf")
